<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="appointmentviewapp.controller.AppointmentListView" displayBlock="true">
<Page id="page" title="{i18n>title}">
    <content>
        	
    <Table id="idAppointmentTable"
		inset="false"
		items="{
			path: '/AppointmentCollection'
		}">
        	
        <items>
        	
            <ColumnListItem type="Active" id="item0">
        	
                <cells>
                
        	
                    <Text text="{Subject}" id="text0"/>
        	
                    <Text text="{StartDate}" id="text1"/>
        	
                    <Text text="{ChangedBy}" id="text2"/></cells>
        	
            </ColumnListItem>
        	
        </items>
        	
        <columns>
        	
            <Column id="column0">
        	
                <header>
        	
                    <Label text="Header 1" id="label0"/>
        	
                </header>
        	
            </Column>
        	
            <Column id="column1">
        	
                <header>
        	
                    <Label text="Header 2" id="label1"/>
        	
                </header>
        	
            </Column>
        	
            <Column id="column2">
        	
                <header>
        	
                    <Label text="Header 3" id="label2"/>
        	
                </header>
        	
            </Column></columns></Table>
        	
    </content>
    </Page>
</mvc:View>